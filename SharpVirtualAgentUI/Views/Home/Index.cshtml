    function GetWeather()
    {
        var url = '@Url.Action("GetWeather", "Home")';
        $.get(url, { sentence: $("#txtSentence").val() }, function (data) { displayData(data); });
    }

    function displayData(data){
        //alert(data.Forecast.Location.Name);
        alert(data.Forecast.Forecast);
        var columns = addAllColumnHeaders(data.Forecast.Forecast);
        for (var i = 0; i < data.Forecast.Forecast.length; i++) {
            var row$ = $('<tr/>');
            for (var colIndex = 0; colIndex < columns.length; colIndex++) {
                var cellValue = data.Forecast.Forecast[i][columns[colIndex]];

                if (cellValue == null) { cellValue = ""; }

                row$.append($('<td/>').html(cellValue));
            }
            $("#jsonTable").append(row$);
        }
    }



    function addAllColumnHeaders(myList) {
        var columnSet = [];
        var headerTr$ = $('<tr/>');

        for (var i = 0; i < myList.length; i++) {
            var rowHash = myList[i];
            for (var key in rowHash) {
                if ($.inArray(key, columnSet) == -1) {
                    columnSet.push(key);
                    headerTr$.append($('<th/>').html(key));
                }
            }
        }
        $("#jsonTable").append(headerTr$);

        return columnSet;
    }


</script>
